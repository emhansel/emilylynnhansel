    img(src='assets/img/dancing/4.png')
    img(src='assets/img/dancing/5.png')
    img(src='assets/img/dancing/6.png')
    img(src='assets/img/dancing/7.png')
    img(src='assets/img/dancing/8.png')
    img(src='assets/img/dancing/9.png')
    img(src='assets/img/dancing/10.png')
    img(src='assets/img/dancing/11.png')
block scripts
  script(src="./assets/js/jquery.js")
  script(src="./assets/js/app.js")
