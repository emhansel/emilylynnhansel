{
  "name": "ELH",
  "version": "0.1.0",
  "description": "Portfolio",
  "main": "index.html",
  "scripts": {
    "sass": "sass --watch sass:assets/css --style compressed",
    "pug": "pug -P --watch .",
    "serve": "serve",
    "dev": "concurrently --kill-others \"npm run serve\" \"npm run sass\" \"npm run pug\"",
    "build": "pug . && npm run sass",
    "start": "echo 'Started!'"
  },
  "keywords": [
    "portfolio"
  ],
  "author": "Michael Fix",
  "dependencies": {
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-marked": "^1.0.2"
  }
}
